<div class="nav">
	<div class="row">
		<div class="column pl2">
			{{#if leftNavItem }}
				{{#if leftNavUrl }}
				<a href="#{{ leftNavUrl }}" data-goto="{{ leftNavUrl }}">
					<p class="bold-text">{{ leftNavItem }}</p>
				</a>
				{{else}}
					<p>{{ leftNavItem }}</p>
				{{/if}}
			{{/if}}
		</div>
		<div class="column center-text pl2">
			{{#if centerNavItem}}
				<p>{{ centerNavItem }}</p>
			{{/if}}
		</div>
		<div class="column right-text pr2">
			{{#if rightNavItem}}
				<p>{{ rightNavItem }}</p>
			{{/if}}
		</div>
	</div>
</div>